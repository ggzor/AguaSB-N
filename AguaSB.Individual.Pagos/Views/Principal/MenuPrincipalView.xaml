             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AguaSB.Individual.Pagos.Views.Principal"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="800">
